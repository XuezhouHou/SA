	publisher = {IEEE},
	author = {De Sousa, Alex Luiz and De Oliveira, Andre Schneider},
	month = nov,
	year = {2020},
	pages = {1--6},
	file = {011_Distributed_MAS_with_Leaderless_Consensus_to_Job-Shop_Scheduler_in_a_Virtual_Smart_Factory_with_Modular_Conveyors.pdf:/home/xuezhou/Documents/SA/Arbeitsvorbereitung/Zotero_paper/011_Distributed_MAS_with_Leaderless_Consensus_to_Job-Shop_Scheduler_in_a_Virtual_Smart_Factory_with_Modular_Conveyors.pdf:application/pdf},
