@inproceedings{fan_hierarchical_2022,
	address = {Xiamen, China},
	title = {A hierarchical structure of cyber-physical production systems based on heterogeneous multi-agent systems},
	isbn = {978-1-66546-533-5},
	url = {https://ieeexplore.ieee.org/document/10055603/},
	doi = {10.1109/CAC57257.2022.10055603},
	abstract = {In the context of Industry 4.0, cyber-physical production systems (CPPS) are fast becoming the critical instrument in the intelligent manufacturing. In reality, there are various heterogeneous objects in the CPPS, including sensors or devices in the physical environment and algorithmic modules in the network space. The ﬁndings of this research provide insights for designing the hierarchical structure of CPPS. The CPPS, which are divided into the physical resources layer, the smart connection layer, the local deployment layer and the cloudPlatform as a Service (cloud-PaaS) layer, can be abstracted into the heterogeneous multi-agent systems (MAS) with the directed topologies, and agents with different functions represent for the corresponding objects in the CPPS. Its strength is in information exchange among different objects in the CPPS via Data Engine (DE) agents. In addition, the communication topology of the CPPS is described as the directed graph. A case study for an unmanned factory is conducted to show the feasibility of the method suggested in this paper. The proposed method could be able to act as a signiﬁcant valuable reference for dealing with the problem of collaborative control in the CPPS.},
	language = {en},
	urldate = {2023-10-17},
	booktitle = {2022 {China} {Automation} {Congress} ({CAC})},
	publisher = {IEEE},
	author = {Fan, Jianjie and Yue, Liang and Zheng, Song},
	month = nov,
	year = {2022},
	pages = {4716--4720},
	file = {010_A_hierarchical_structure_of_cyber-physical_production_systems_based_on_heterogeneous_multi-agent_systems.pdf:/home/xuezhou/Documents/SA/Arbeitsvorbereitung/Zotero_paper/010_A_hierarchical_structure_of_cyber-physical_production_systems_based_on_heterogeneous_multi-agent_systems.pdf:application/pdf},
